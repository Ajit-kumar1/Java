package Programs;
import java.util.*;
class Person{
	Scanner sc=new Scanner(System.in);
	String name;//="Divya";
	int age;
}
class Teacher extends Person{

	int teacher_id,salary;
	String college;
	String desti;
	void input() {
		name=sc.nextLine();
		age=sc.nextInt();
		teacher_id=sc.nextInt();
		salary=sc.nextInt();
		college=sc.next();
		desti=sc.next();
	}
	void show() {
		System.out.println("Name is "+name);
		System.out.println("age is "+age);
		System.out.println("Teacher id is "+teacher_id);
		System.out.println("salary is "+salary);
		System.out.println("College is "+college);
		System.out.println("destination is "+desti);
		
	}
}
public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Teacher ob=new Teacher();
		ob.input();
		ob.show();
	}

}
